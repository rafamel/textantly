{
  "name": "textantly",
  "version": "0.0.1",
  "description": "Delicious morning coffee. A keystroke away.",
  "scripts": {
    "build": "npm run lint && webpack",
    "dev": "ENV=development webpack-dev-server --hot",
    "lint": "eslint ./src --ext .js",
    "lint:test": "eslint ./test --ext .js",
    "lint:md": "markdownlint *.md --config markdown.json",
    "test": "npm run lint:test && jest ./test/.*.test.js --runInBand --verbose",
    "checks": "run-s lint lint:* && jest ./test/.*.test.js",
    "update:packages": "rm -rf node_modules && rm package-lock.json && npm update --save/save-dev && npm install && npm outdated"
  },
  "author": "Rafa Mel",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "bootstrap": "^3.3.7",
    "caman": "^4.1.2",
    "canvas": "^1.6.7",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "jest-cli": "^21.2.1",
    "jquery": "^3.2.1",
    "markdownlint-cli": "^0.5.0",
    "nodemon": "^1.12.1",
    "nouislider": "^9.0.0",
    "npm-run-all": "^4.1.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "collectCoverage": true
  }
}
