{
  "name": "textantly",
  "version": "0.0.1",
  "description": "Delicious morning coffee. A keystroke away.",
  "private": true,
  "scripts": {
    "start": "node util/daemon --rundaemon \"react-app-rewired start\" --watch ./src --run \"npm run lint\"",
    "build": "npm run checks && react-app-rewired build",
    "lint": "eslint ./src --ext .js",
    "lint:test": "eslint ./test --ext .js",
    "lint:md": "markdownlint *.md --config markdown.json",
    "test": "npm run lint:test && react-app-rewired test --env=jsdom --coverage",
    "test:watch": "nodemon --watch test --exec \"clear && npm t\"",
    "checks": "run-s lint lint:* react-app-rewired test --env=jsdom --coverage",
    "update:packages": "rm -rf node_modules && rm package-lock.json && npm update --save/save-dev && npm install && npm outdated",
    "eject": "react-scripts eject"
  },
  "author": "Rafa Mel",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootstrap-material-design": "^0.5.10",
    "caman": "^4.1.2",
    "classnames": "^2.2.5",
    "clear": "0.0.1",
    "core-js": "^2.5.3",
    "font-awesome": "^4.7.0",
    "fs-readdir-recursive": "^1.1.0",
    "jquery": "^3.2.1",
    "material-design-icons": "^3.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-file-reader": "^1.1.3",
    "typeface-playfair-display-sc": "0.0.45",
    "typeface-roboto": "0.0.45"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/react": "^16.0.31",
    "babel-eslint": "^8.0.3",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.3.0",
    "chokidar": "^1.7.0",
    "eslint": "^4.13.1",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-react-app": "^1.0.2",
    "eslint-plugin-standard": "^3.0.1",
    "imports-loader": "^0.7.1",
    "lodash.merge": "^4.6.0",
    "markdownlint-cli": "^0.5.0",
    "nodemon": "^1.13.3",
    "npm-run-all": "^4.1.2",
    "react-app-rewire-eslint": "^0.2.3",
    "react-app-rewire-polyfills": "^0.2.0",
    "react-app-rewired": "^1.4.0",
    "react-scripts": "^1.0.17",
    "yargs": "^10.0.3"
  }
}
