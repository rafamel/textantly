{
  "name": "textantly",
  "version": "0.0.1",
  "description": "Delicious morning coffee. A keystroke away.",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "dev": "node utils/daemon --rundaemon \"npm start\" --watch ./src --run \"npm run lint\"",
    "prebuild": "node ./utils/prebuild.js",
    "build": "npm run checks && react-app-rewired build",
    "analyze": "source-map-explorer build/static/js/main.*",
    "deploy": "npm run build && surge -d https://textantly.surge.sh -p ./build",
    "lint": "eslint ./src --ext .js",
    "lint:test": "eslint ./test --ext .js",
    "lint:utils": "eslint ./utils --ext .js",
    "lint:md": "markdownlint *.md --config markdown.json",
    "test": "npm run lint:test && react-app-rewired test --env=jsdom --coverage",
    "test:watch": "nodemon --watch test --exec \"clear && npm t\"",
    "checks": "run-s lint lint:* && react-app-rewired test --env=jsdom --coverage",
    "update:packages": "rm -rf node_modules && rm package-lock.json && npm update --save/save-dev && npm install && npm outdated"
  },
  "author": "Rafa Mel",
  "dependencies": {
    "classnames": "^2.2.5",
    "cropperjs": "^1.2.2",
    "deep-diff": "^0.3.8",
    "dotenv": "^5.0.0",
    "font-awesome": "^4.7.0",
    "fontfaceobserver": "^2.0.13",
    "google-fonts": "^1.0.0",
    "html2canvas": "^1.0.0-alpha.9",
    "keen-tracking": "^1.4.0",
    "localforage": "^1.5.6",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.values": "^4.3.0",
    "material-ui": "^1.0.0-beta.31",
    "material-ui-icons": "^1.0.0-beta.17",
    "rc-collapse": "^1.7.7",
    "rc-slider": "^8.5.0",
    "react": "^16.2.0",
    "react-broadcast": "^0.6.1",
    "react-collapse": "^4.0.3",
    "react-cropper": "^1.0.1",
    "react-dom": "^16.2.0",
    "react-jss": "^8.2.0",
    "react-motion": "^0.5.2",
    "react-redux": "^5.0.6",
    "react-swipeable-views": "^0.12.12",
    "redux": "^3.7.2",
    "redux-logic": "^0.12.3",
    "redux-persist": "^5.5.0",
    "reselect": "^3.0.1",
    "resize-observer-polyfill": "^1.5.0",
    "typeface-playfair-display-sc": "0.0.54",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "@types/jest": "^22.1.0",
    "@types/react": "^16.0.31",
    "babel": "^6.23.0",
    "babel-eslint": "^8.0.3",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.3.0",
    "chokidar": "^2.0.0",
    "clear": "0.0.1",
    "eslint": "^4.13.1",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-react-app": "^1.0.2",
    "eslint-plugin-standard": "^3.0.1",
    "google-fonts-complete": "^1.2.1",
    "markdownlint-cli": "^0.7.0",
    "nodemon": "^1.13.3",
    "npm-run-all": "^4.1.2",
    "react-app-rewired": "^1.4.0",
    "react-scripts": "^1.0.17",
    "source-map-explorer": "^1.5.0",
    "surge": "^0.19.0",
    "yargs": "^11.0.0"
  }
}
